{
  "name": "Fleury Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#020202",
    "editor.foreground": "#b99468",
    "editorLineNumber.foreground": "#404040",
    "editorLineNumber.activeForeground": "#808080",
    "editor.lineHighlightBackground": "#1E1E1E",
    "editor.selectionBackground": "#494949",
    "editor.selectionHighlightBackground": "#49494966",
    "editorCursor.foreground": "#00EE00",
    "editor.findMatchBackground": "#303040",
    "editor.findMatchHighlightBackground": "#30304044",
    "editorBracketMatch.background": "#8ffff244",
    "editorBracketMatch.border": "#8ffff2",

    "sideBar.background": "#222425",
    "sideBar.foreground": "#b99468",
    "sideBarSectionHeader.background": "#362e25",
    "list.hoverBackground": "#362e25",
    "list.activeSelectionBackground": "#63523d",
    "list.inactiveSelectionBackground": "#362e25",
    "list.highlightForeground": "#ffa900",

    "activityBar.background": "#020202",
    "activityBar.foreground": "#fcaa05",
    "activityBarBadge.background": "#de8150",
    "activityBarBadge.foreground": "#FFFFFF",

    "statusBar.background": "#000000",
    "statusBar.foreground": "#fcaa05",
    "statusBar.noFolderBackground": "#000000",
    "statusBar.debuggingBackground": "#de8150",
    "statusBar.debuggingForeground": "#FFFFFF",

    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#fcaa05",
    "titleBar.inactiveBackground": "#020202",

    "tab.activeBackground": "#020202",
    "tab.inactiveBackground": "#222425",
    "tab.activeForeground": "#b99468",
    "tab.inactiveForeground": "#666666",
    "tab.border": "#000000",
    "editorGroupHeader.tabsBackground": "#222425",

    "button.background": "#de8150",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#de8150CC",

    "input.background": "#222425",
    "input.foreground": "#b99468",
    "input.border": "#63523d",
    "inputOption.activeBorder": "#de8150",

    "dropdown.background": "#222425",
    "dropdown.foreground": "#b99468",
    "dropdown.border": "#63523d",

    "focusBorder": "#de8150",
    "widget.shadow": "#000000",

    "panel.background": "#020202",
    "panel.border": "#63523d",
    "panelTitle.activeForeground": "#b99468",
    "panelTitle.inactiveForeground": "#666666",

    "terminal.background": "#020202",
    "terminal.foreground": "#b99468",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#FF0000",
    "terminal.ansiGreen": "#2ab34f",
    "terminal.ansiYellow": "#ffa900",
    "terminal.ansiBlue": "#2895c7",
    "terminal.ansiMagenta": "#FF44DD",
    "terminal.ansiCyan": "#00DDEE",
    "terminal.ansiWhite": "#666666",
    "terminal.ansiBrightBlack": "#404040",
    "terminal.ansiBrightRed": "#db2828",
    "terminal.ansiBrightGreen": "#6eb535",
    "terminal.ansiBrightYellow": "#ffa900",
    "terminal.ansiBrightBlue": "#2895c7",
    "terminal.ansiBrightMagenta": "#FF44DD",
    "terminal.ansiBrightCyan": "#00DDEE",
    "terminal.ansiBrightWhite": "#FFFFFF",

    "gitDecoration.modifiedResourceForeground": "#ffa900",
    "gitDecoration.deletedResourceForeground": "#FF0000",
    "gitDecoration.untrackedResourceForeground": "#6eb535",
    "gitDecoration.conflictingResourceForeground": "#FF44DD",

    "editorGutter.modifiedBackground": "#ffa900",
    "editorGutter.addedBackground": "#6eb535",
    "editorGutter.deletedBackground": "#FF0000",

    "diffEditor.insertedTextBackground": "#2ab34f33",
    "diffEditor.removedTextBackground": "#db282833",

    "editorError.foreground": "#FF0000",
    "editorWarning.foreground": "#ffa900",
    "editorInfo.foreground": "#2895c7",

    "notifications.background": "#222425",
    "notifications.foreground": "#b99468",
    "notificationCenterHeader.background": "#362e25",

    "breadcrumb.foreground": "#666666",
    "breadcrumb.focusForeground": "#b99468",
    "breadcrumb.activeSelectionForeground": "#ffa900",

    "settings.headerForeground": "#b99468",
    "settings.modifiedItemIndicator": "#ffa900"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.quoted.docstring.multi",
        "punctuation.definition.string",
        "string.quoted.single",
        "constant.character",
        "markup.inline.raw",
        "markup.raw",
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#ffa900"
      }
    },
    {
      "name": "Boolean Constants",
      "scope": [
        "constant.language.boolean",
        "constant.language.true",
        "constant.language.false"
      ],
      "settings": {
        "foreground": "#2895c7"
      }
    },
    {
      "name": "Constants and Literals",
      "scope": [
        "constant.numeric",
        "constant",
        "constant.language",
        "support.constant",
        "constant.other"
      ],
      "settings": {
        "foreground": "#ffa900"
      }
    },
    {
      "name": "Import Keywords",
      "scope": [
        "keyword.control.import",
        "keyword.control.include",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Import Module Names",
      "scope": [
        "meta.statement.import entity.name",
        "meta.statement.import support.type",
        "meta.statement.import variable",
        "entity.name.import",
        "entity.name.package",
        "entity.name.module",
        "support.other.module",
        "meta.import variable.other",
        "meta.preprocessor.include string",
        "meta.preprocessor.include punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#dc7575"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Preprocessor",
      "scope": [
        "meta.preprocessor",
        "keyword.control.directive",
        "punctuation.definition.directive"
      ],
      "settings": {
        "foreground": "#dc7575"
      }
    },
    {
      "name": "Function Name",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#de451f"
      }
    },
    {
      "name": "Default Text and Variables",
      "scope": [
        "meta.function-call",
        "meta.function-call.generic",
        "variable",
        "variable.other",
        "support.variable",
        "variable.parameter",
        "markup.list",
        "punctuation.definition.list_item",
        "meta.function",
        "meta.class",
        "source",
        "text"
      ],
      "settings": {
        "foreground": "#b99468"
      }
    },
    {
      "name": "Python Self",
      "scope": [
        "variable.parameter.function.language.special.self.python"
      ],
      "settings": {
        "foreground": "#dc7575"
      }
    },
    {
      "name": "Python Decorator",
      "scope": [
        "meta.function.decorator.python",
        "entity.name.function.decorator.python"
      ],
      "settings": {
        "foreground": "#de451f"
      }
    },
    {
      "name": "Built-in Functions and Types (like macros)",
      "scope": [
        "support.function.builtin",
        "support.type.builtin",
        "support.function.magic",
        "entity.name.function.preprocessor"
      ],
      "settings": {
        "foreground": "#2895c7"
      }
    },
    {
      "name": "Type",
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.enum",
        "entity.name.interface",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#edb211"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#dc7575"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#bd2d2d"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.bracket",
        "meta.delimiter"
      ],
      "settings": {
        "foreground": "#7a6b5a"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF0000",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Diff Header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#2895c7"
      }
    },
    {
      "name": "Diff Inserted",
      "scope": [
        "markup.inserted",
        "meta.diff.inserted"
      ],
      "settings": {
        "foreground": "#2ab34f"
      }
    },
    {
      "name": "Diff Deleted",
      "scope": [
        "markup.deleted",
        "meta.diff.deleted"
      ],
      "settings": {
        "foreground": "#FF0000"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "entity.name.section"
      ],
      "settings": {
        "foreground": "#f0c674",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": [
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#2895c7"
      }
    },
    {
      "name": "Markup Quote",
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#666666",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid Deprecated",
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#db2828",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "String Regexp",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#bd2d2d"
      }
    },
    {
      "name": "Character Escape",
      "scope": [
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#bd2d2d"
      }
    },
    {
      "name": "Variable Language",
      "scope": [
        "variable.language"
      ],
      "settings": {
        "foreground": "#dc7575"
      }
    },
    {
      "name": "Keyword Other",
      "scope": [
        "keyword.other"
      ],
      "settings": {
        "foreground": "#f0c674"
      }
    },
    {
      "name": "Markup Underline",
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "function": "#de451f",
    "function.defaultLibrary": "#2895c7",
    "method.defaultLibrary": "#2895c7",
    "type": "#edb211",
    "type.defaultLibrary": "#2895c7",
    "enum": "#a7eb13",
    "parameter": "#b99468",
    "parameter.declaration": "#c9598a",
    "variable": "#b99468",
    "variable.declaration": "#c9598a",
    "variable.defaultLibrary": "#2895c7",
    "property": "#b99468",
    "property.declaration": "#c9598a",
    "enumMember": "#6eb535",
    "namespace": "#dc7575",
    "module": "#dc7575",
    "keyword": "#f0c674",
    "comment": "#666666",
    "string": "#ffa900",
    "number": "#ffa900",
    "operator": "#bd2d2d",
    "decorator": "#2895c7",
    "function.decorator": "#2895c7",
    "macro": "#2895c7",
    "magicFunction": "#2895c7",
    "builtinConstant": "#2895c7"
  }
}